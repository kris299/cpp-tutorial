#include <iostream>
using namespace std;

int main()
{
	// 2D-arrays are straight forward to initialize
	int array[5][5] = {
		{0,1,2,3,4},
		{2,3,4,5,6},
		{5,3,2,4,5},
		{5,3,4,5,6},
		{2,4,5,6,7}
	};

	for (int i=0; i<5; ++i)
	{
		for (int j=0; j<5; ++j)
		{
			cout << array[i][j] << " ";
		}
		cout << endl;
	}
	return 0;
}
